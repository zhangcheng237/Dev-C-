#include<iostream>
#include<algorithm>
using namespace std;
int main()
{
	int u=0;
	while(u<=20){
	int a,b,c=0,d;
	cin>>a>>b;
	int max_ab=max(a,b);
	if(a>b)
		c++;
	if(a<b)
		d++;
	if(max_ab==a)
		cout<<"a"<<" "<<c<<":"<<d<<" "<<"b";
		cout<<" ";
		u++;
	if(max_ab==b)
		cout<<"a"<<" "<<c<<":"<<d<<" "<<"b";
		u++;
	}
	return 0;
}
